{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET","OPTIONS","PATCH","DELETE","POST","PUT"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
